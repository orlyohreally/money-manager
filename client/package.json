{
  "name": "money-manager-client",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test:unit-test": "ng test --watch=false",
    "test:e2e": "ng e2e",
    "test": "npm run test:unit-test && npm run test:e2e",
    "start:dev": "ng serve --proxy-config proxy.conf.json",
    "postinstall": "ng build --aot --prod",
    "precommit": "lint-staged",
    "setup-webdriver-manager": "cd ./node_modules/protractor/bin && webdriver-manager update --versions.chrome=80.0.3987.163"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/cli": "~7.3.6",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/material-moment-adapter": "^9.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "dotenv": "^8.2.0",
    "hammerjs": "^2.0.8",
    "jasmine-marbles": "^0.6.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine2-protractor-utils": "^1.3.0",
    "lint-staged": "^10.1.7",
    "moment": "^2.24.0",
    "ng-lint-staged": "^0.1.6",
    "ng-mocks": "^8.1.0",
    "ngx-device-detector": "^1.3.5",
    "ngx-image-cropper": "^1.3.10",
    "ngx-localstorage": "^2.3.0",
    "ngx-mat-select-search": "^1.8.0",
    "protractor-html-reporter-2": "^1.0.4",
    "protractor-http-client": "^1.0.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.10.0",
    "typescript": "~3.2.2",
    "webdriver-manager": "^12.1.7",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.126",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^3.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-parallel": "^0.3.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.11.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "lint-staged": {
    "**/*.+(js|ts|json|css)": [
      "pretty-quick --staged"
    ],
    "src/**/*.ts": [
      "ng-lint-staged lint money-manager --fix --"
    ],
    "e2e/**/*.ts": [
      "ng-lint-staged lint money-manager-e2e --fix --"
    ]
  }
}