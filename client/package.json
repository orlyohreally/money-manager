{
  "name": "money-manager-client",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "test:unit-test": "ng test --watch=false",
    "test:e2e": "ng e2e --configuration=staging",
    "server:test": "cd ../server && npm run start:test",
    "e2e": "start-server-and-test server:test 3000 test:e2e",
    "start:dev": "ng serve --proxy-config proxy.conf.json",
    "precommit": "lint-staged",
    "postinstall": "ng build --aot --prod",
    "setup-webdriver-manager": "cd ./node_modules/protractor/bin && webdriver-manager update --versions.chrome=80.0.3987.163"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.901.10",
    "@angular-material-components/datetime-picker": "^2.0.4",
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/cli": "~9.1.10",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~9.1.11",
    "@angular/localize": "^9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "angular-google-charts": "^1.1.4",
    "codelyzer": "^6.0.0",
    "core-js": "^2.5.4",
    "dotenv": "^8.2.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-reporters": "^2.5.0",
    "jasmine2-protractor-utils": "^1.3.0",
    "lint-staged": "^10.1.7",
    "moment": "^2.24.0",
    "ng-lint-staged": "^0.1.6",
    "ng-mocks": "^8.1.0",
    "ng-recaptcha": "^5.0.0",
    "ngx-device-detector": "^1.4.5",
    "ngx-image-cropper": "^1.3.10",
    "ngx-localstorage": "^2.3.0",
    "ngx-mat-select-search": "^3.0.0",
    "protractor-html-reporter-2": "^1.0.4",
    "protractor-http-client": "^1.0.4",
    "rxjs": "~6.6.0",
    "start-server-and-test": "^1.11.0",
    "tslib": "^1.10.0",
    "typescript": "~3.8.3",
    "webdriver-manager": "^12.1.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/language-service": "~9.1.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.126",
    "@types/node": "^12.11.1",
    "husky": "^4.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-parallel": "^0.3.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~5.11.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0"
  },
  "lint-staged": {
    "**/*.+(js|ts|json|css)": [
      "pretty-quick --staged"
    ],
    "src/**/*.ts": [
      "ng-lint-staged lint money-manager --fix --"
    ],
    "e2e/**/*.ts": [
      "ng-lint-staged lint money-manager-e2e --fix --"
    ]
  }
}
