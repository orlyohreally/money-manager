{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      ["app", "shared", "family", "payment"],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "app",
        "auth",
        "nav",
        "shared",
        "family",
        "home",
        "payment",
        "member",
        "user",
        "tc",
        "about",
        "blog"
      ],
      "kebab-case"
    ]
  }
}
