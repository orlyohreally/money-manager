{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      [
        "app",
        "shared"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "app",
        "auth",
        "nav",
        "shared",
        "family",
        "home",
        "payment"
      ],
      "kebab-case"
    ]
  }
}
