  },
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "engines": {
    "node": "~10.15.3",
    "npm": "^6.9.0"
  },
  "dependencies": {
    "@angular/cli": "~7.3.6",
    "lint-staged": "^10.2.2",
    "npm": "^6.9.0"
  },
  "devDependencies": {
    "lerna": "^3.20.2"
  },
  "author": "Orly Knop",
