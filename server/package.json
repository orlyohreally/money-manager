{
  "name": "money-manager-server",
  "version": "1.0.0",
  "description": "NodeJS server for money-manager project",
  "main": "index.js",
  "engines": {
    "node": "~10.15.3",
    "npm": "^6.9.0"
  },
  "scripts": {
    "build": "npm run setup-swagger && tsc",
    "build:clean": "npm run clean && npm run build",
    "watch": "npm run setup-swagger && tsc -w",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "npm run setup-swagger && tsc -p tsconfig.json --noEmit",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "start": "node src/main.js",
    "start:test": "npm run build:clean && env NODE_ENV=testing node src/test-server.js",
    "start:dev": "concurrently --kill-others \"npm run setup-swagger && tsc -w\" \"set DEBUG=* && nodemon --inspect src/main.js\"",
    "test": "jest --detectOpenHandles",
    "precommit": "lint-staged && npm run lint",
    "test:unit": "jest --detectOpenHandles --testPathPattern \"src/.+spec.tsx?$\"",
    "test:e2e": "jest --detectOpenHandles --testPathPattern --runInBand \"__tests__/.+.tsx?\" --forceExit",
    "test:ci": "jest --detectOpenHandles --coverage",
    "clean": "rimraf \"{src,__tests__}/**/*.js?(x)?(.map)\"",
    "release": "./extra/deployment/release.sh",
    "setup-swagger": "multi-file-swagger src/services/api-documentation/swagger.json >  src/services/api-documentation/options.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orlyohreally/money-manager.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Orly Knop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orlyohreally/money-manager/issues"
  },
  "homepage": "https://github.com/orlyohreally/money-manager#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "express-swagger-generator": "^1.1.17",
    "jest": "^23.6.0",
    "mongodb-memory-server": "^6.5.2",
    "nodemon": "^1.18.10",
    "prettier-config-trucknet-io": "^2.0.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^23.10.3",
    "tslint": "^5.12.1",
    "tslint-config-trucknet-io": "^1.4.0"
  },
  "_moduleAliases": {
    "@src": "src",
    "@shared": "../shared"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@sendgrid/mail": "^6.4.0",
    "@types/atob": "^2.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.16.1",
    "@types/hapi__joi": "^16.0.3",
    "@types/jest": "^23.3.13",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.130",
    "@types/mongoose": "^5.5.1",
    "@types/node": "^10.12.21",
    "@types/redis": "^2.8.14",
    "@types/redis-mock": "^0.17.0",
    "@types/supertest": "^2.0.9",
    "atob": "^2.1.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.2.0",
    "email-check": "^1.1.0",
    "express": "^4.17.0",
    "express-recaptcha": "^5.0.2",
    "http-status-codes": "^1.4.0",
    "husky": "^4.2.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.13",
    "module-alias": "^2.1.0",
    "mongoose": "^5.7.5",
    "multi-file-swagger": "^2.3.0",
    "node-mocks-http": "^1.8.1",
    "npm": "^6.14.6",
    "redis": "^2.8.0",
    "redis-mock": "^0.49.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.1"
  }
}