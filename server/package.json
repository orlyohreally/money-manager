{
  "name": "money-manager-server",
  "version": "1.0.0",
  "description": "NodeJS server for money-manager project",
  "main": "index.js",
  "engines": {
    "node": "~10.15.3",
    "npm": "~6.9.0"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "npm run clean && npm run build",
    "watch": "tsc -w",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint:prettier": "prettier --write \"{src,test}/**/*.ts*\"",
    "start": "node src/main.js",
    "start:dev": "concurrently --kill-others \"tsc -w\" \"set DEBUG=* && nodemon --inspect src/main.js\"",
    "commit": "commit-prompt --no-verify",
    "precommit": "lint-staged && npm run lint",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --detectOpenHandles --testPathPattern \"src/.+spec.tsx?$\"",
    "test:e2e": "jest --detectOpenHandles --testPathPattern \"__tests__/.+.tsx?\"",
    "test:ci": "jest --detectOpenHandles --coverage",
    "clean": "rimraf \"{src,__tests__}/**/*.js?(x)?(.map)\"",
    "release": "./extra/deployment/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orlyohreally/money-manager.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Orly Knop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orlyohreally/money-manager/issues"
  },
  "homepage": "https://github.com/orlyohreally/money-manager#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "prettier-config-trucknet-io": "^2.0.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "trucknet-commit": "^1.4.3",
    "ts-jest": "^23.10.3",
    "tslint": "^5.12.1",
    "tslint-config-trucknet-io": "^1.4.0"
  },
  "_moduleAliases": {
    "@src": "src",
    "@shared": "../shared"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@sendgrid/mail": "^6.4.0",
    "@types/atob": "^2.1.2",
    "@types/bcrypt": "^3.0.0",
    "@types/hapi__joi": "^16.0.3",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mongoose": "^5.5.1",
    "@types/node": "^10.12.21",
    "@types/redis": "^2.8.14",
    "atob": "^2.1.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "crypto-random-string": "^3.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.13",
    "module-alias": "^2.1.0",
    "mongoose": "^5.7.5",
    "npm": "^6.9.0",
    "redis": "^2.8.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.130",
    "node-mocks-http": "^1.8.1"
  }
}